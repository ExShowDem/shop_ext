<?xml version="1.0" encoding="utf-8"?>
<form>
    <fieldset addfieldpath="/libraries/redcore/form/fields">
        <field
                name="id"
                type="hidden"
                default="0"
                label="COM_REDCORE_ID_LABEL"
                description="COM_REDCORE_ID_DESC"
                readonly="true"
                />
        <field
                name="shipping_name"
                type="shippingplugins"
                label="COM_REDSHOPB_SHIPPING_CONFIGURATION_SHIPPING_NAME_LABEL"
                description="COM_REDSHOPB_SHIPPING_CONFIGURATION_SHIPPING_NAME_DESC"
                required="true"
                >
        </field>
        <field
                name="state"
                labelclass="col-sm-2 control-label"
                type="radio"
                label="JSTATUS"
                description="COM_REDSHOPB_SHIPPING_CONFIGURATION_STATE_DESC"
                required="true"
                class="btn-group"
                default="1"
                >
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
        </field>
        <field
                name="extension_name"
                type="hidden"
                default="com_redshopb"
                required="true"
                />
        <field
                name="owner_name"
                type="hidden"
                />
    </fieldset>
    <fields name="params">
    </fields>
</form>
